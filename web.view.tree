$mol_hint $mol_check
	dictionary *
	Icon $mol_icon_help_circle_outline
	hint_close @ \Hide
	hint_open @ \Hint

$mol_scroll $mol_view
	scroll_top? 0
	scroll_left? 0
	attr *
		^
		tabindex <= tabindex -1
	event *
		^
		scroll?event <=> event_scroll?event null

$mol_page $mol_view
	dom_name \article
	attr *
		^
		tabIndex <= tabindex -1
	sub /
		<= Head $mol_view
			minimal_height 64
			dom_name \header
			sub <= head /
				<= Title $mol_view
					dom_name \h1
					sub <= title_content /
						<= Logo null
						<= title
				<= Tools $mol_view
					sub <= tools /$mol_view_content
		<= Body $mol_scroll
			scroll_top? => body_scroll_top?
			sub <= body_content /
				<= Body_content $mol_view
					sub <= body /$mol_view
		<= Foot $mol_view
			dom_name \footer
			sub <= foot /$mol_view

$mol_row $mol_view

$mol_form $mol_list
	submit_allowed true
	submit_blocked false
	event *
		^
		keydown?event <=> keydown?event null
	submit?event null
	rows /
		<= Body $mol_list
			sub <= body <= form_fields /$mol_form_field
		<= Foot $mol_row
			sub <= foot <= buttons /$mol_view

$hyoo_password $mol_view
	plugins /
		<= Theme $mol_theme_auto
	sub /
		<= Page $mol_page
			title <= title @ \Password generator
			Logo <= Logo $mol_avatar
				id <= master
			tools /
				<= Source $mol_link_source
					uri \https://github.com/hyoo-ru/password.hyoo.ru
				<= Lights $mol_lights_toggle
				<= Close $mol_button_minor
					hint @ \Clear and close
					click? <=> close? null
					sub /
						<= Close_icon $mol_icon_close
			body <= fields /
				<= Master_block $mol_form_field
					name @ \Master password
					Content <= Master $mol_password
						value? <=> master? \
				<= Context_block $mol_form_field
					name @ \Domain or App name
					Content <= Context_bar $mol_bar sub /
						<= Context $mol_string
							value? <=> context? \
						<= Context_clear $mol_button_minor
							hint @ \Clear
							click? <=> context_clear? null
							sub /
								<= Clear_icon $mol_icon_trash_can_outline
				<= Password_block $mol_form_field
					name @ \Unique strong password
					Content <= Password_bar $mol_bar sub /
						<= Index $mol_paginator
							value? <=> index? 0
						<= Copy $mol_button_copy
							text <= password \
						<= Password $mol_password
							value <= password
				<= Hint $mol_hint
					dictionary *
						no_store @ \Passwords don't stored anywhere
						offline @ \Working instantly offline
						strong @ \Master password must be strong
